{
	$basetexture "models/weapons/v_models/sleeves/sleeves_diffuse"
	$bumpmap "models/weapons/v_models/sleeves/sleeves_normalspec"

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.25 0.25 0.25]"
	$envmapcontrast 0.76
	$envmapsaturation 0.22
	$envmapfresnel 1
	$envmaplightscale 0.4
	$envmaplightscaleminmax "[0 1.10]"
